{
  "name": "4rpl-lang",
  "displayName": "4rpl-lang",
  "description": "VSCode syntax highlighting for 4RPL",
  "publisher": "Up-Level",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.76.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Up-Level/4rpl-lang.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "4rpl",
        "aliases": [
          "4RPL",
          "4rpl"
        ],
        "extensions": [
          "4rpl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "4rpl",
        "scopeName": "source.4rpl",
        "path": "./syntaxes/4rpl.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "process-commands": "node out/process-commands.js"
  },
  "devDependencies": {
    "@types/node": "^8.10.25",
    "@types/vscode": "^1.51.0",
    "@types/axios": "^0.14.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "node-html-parser": "^6.1.5"
  }
}
