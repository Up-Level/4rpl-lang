{
  "name": "4rpl-lang",
  "displayName": "4rpl-lang",
  "description": "VSCode Language support for Creeper World 4's 4RPL.",
  "publisher": "Up-Level",
  "version": "0.5.0",
  "engines": {
    "vscode": "^1.71.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Up-Level/4rpl-lang.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "activationEvents": [],
  "contributes": {
    "languages": [
      {
        "id": "4rpl",
        "aliases": [
          "4RPL",
          "4rpl"
        ],
        "extensions": [
          "4rpl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "4rpl",
        "scopeName": "source.4rpl",
        "path": "./syntaxes/4rpl.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "4rpl-lang.read-game-output",
        "title": "4rpl-lang: Read Game Output"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^8.10.25",
    "@types/vscode": "^1.71.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
